    "test": "eslint ./src/**/*.spec.js & mocha ./src/**/*.spec.js --require @babel/register --require ./test/helpers.js --reporter spec || exit 0",
    "lint": "eslint './src/**/*.js'",
    "coveralls": "cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "recipes"
  ],
  "author": "Nadine Kinnen",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "7.12.10",
