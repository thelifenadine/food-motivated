    "mocha": "8.2.1",
    "nodemon": "2.0.7",
    "nyc": "15.1.0",
    "proxyquire": "2.1.3",
    "redux-devtools-extension": "2.13.8",
    "redux-logger": "3.0.6",
    "sinon": "9.2.3",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-middleware": "3.7.2",
