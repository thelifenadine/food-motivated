{
  "name": "food-motivated",
  "version": "1.2.1",
  "description": "raw dog food recipe calculator",
  "repository": "github:thelifenadine/food-motivated",
  "private": true,
  "scripts": {
    "coverage": "nyc npm run test",
    "start": "webpack-dev-server --open",
    "build-prod": "webpack --config webpack.prod.js",
